    "import retro\n",
    "\n",
    "env = retro.make(game='ContraForce-Nes')\n",
    "obs = env.reset()\n",
    "for _ in range(1000):\n",
    "    obs, rew, done, info = env.step(env.action_space.sample())\n",
    "    env.render()\n",
    "    if done:\n",
    "        obs = env.reset()\n",
    "env.close()"
   ]
